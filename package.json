{
  "name": "socket-app",
  "version": "0.0.1",
  "license": "ISC",
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "concurrently -n \"proxy,server\" -c \"green.bold,yellow.bold\" \"npm run client:start\" \"npm run server:start\" ",
    "client:start": "cd socket-app && ng serve --open",
    "server:start": "node --inspect=5858 -r ts-node/register ./socket-server/src/app.ts",
    "build": "cd socket-app && ng build",
    "test": "cd socket-app && ng test",
    "lint": "cd socket-app && ng lint",
    "e2e": "cd socket-app && ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "express": "~4.17.1",
    "ngx-socket-io": "~2.1.1",
    "rxjs": "~6.5.2",
    "socket.io": "~2.2.0",
    "tslib": "~1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.8",
    "@types/socket.io": "^2.1.0",
    "codelyzer": "^5.0.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.17.0",
    "typescript": ">=3.4 <3.5"
  }
}
